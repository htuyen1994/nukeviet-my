			<script language="javascript">
			
            	function send_info()
				{
					check_form("contact");
					var contact_realname = document.getElementById("contact_realname").value;
					var contact_email = document.getElementById("contact_email").value;
					var contact_address = document.getElementById("contact_address").value;
					var contact_phone = document.getElementById("contact_phone").value;
					var contact_region = document.getElementById("contact_region").value;	
					var contact_template = document.getElementById("contact_template").value;
					var contact_content = document.getElementById("contact_content").value;
					var security_code = document.getElementById("security_code").value;
					var domain = document.getElementById("domain").value;
					
			
					// Check domain trial
					if (domain)
					{
						AjaxRequest.get(
						{
							'url': site_root_domain+"/cron/cron.php?mod=check_domain_trial&key_web=610a3a34e2dcec65537a98d0132f9690&text="+domain
							,'onSuccess':function(req)
							{
								if(req.responseText == 1) //OK
								{
									alert('Tên miền dùng thử này đã tồn tại, vui lòng chọn tên miền khác'); 
									document.getElementById('domain').value = '';
									return false;
								}else
								{
									var info_contact = "contact_realname="+contact_realname+"&contact_email="+contact_email+"&contact_address="+contact_address+"&contact_phone="+contact_phone+"&contact_region="+contact_region+"&contact_template="+contact_template+"&contact_content="+contact_content+"&security_code="+security_code+"&domain="+domain;
									
									if(contact_realname != "" && contact_email != "" && contact_address != "" && contact_phone!= "" && security_code!= "")
									{
										AjaxRequest.get(
										{
											'url': site_root_domain+"/?site=contact&view=trial&"+info_contact
									
											,'onLoading':function(req)
											{
												document.getElementById("contact").innerHTML = "<div class='content_loading'><img class='loading' align='absmiddle' src='"+site_img_url+"/icons/ajax_loader.gif'/></div>";
												
											}
											,'onSuccess':function(req)
											{
												//alert(req.responseText); return false;
												if(req.responseText == 1)
												{
													//document.getElementById("loading_result").innerHTML = "";
													alert("Bạn đã gửi thành công! Kiểm tra hộp thư Email hoặc Spam để nhận thông tin web dùng thử !");
													window.location.href= site_root_domain;
												}
												else
												{
													alert("Không thành công!");
													window.location.href= site_root_domain;	
												}
											}
										}
										);
									}	
								}
							}
						}
						);
					}
					
					
					
					
				}
			
            </script>

